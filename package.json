{
  "name": "ngx-src",
  "description": "Mobile web UI based on Angular and Weui",
  "repository": {
    "type": "git",
    "url": "https://github.com/doxiaodong/ngx"
  },
  "version": "1.0.0-pre.7",
  "license": "MIT",
  "scripts": {
    "tslint": "tslint",
    "lint": "npm run tslint \"src/**/*.ts\" && npm run tslint \"server/**/*.ts\"",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "build": "npm run clean && webpack --config config/webpack.prod.js --progress --profile --bail",
    "clean": "rm -rf docs",
    "start": "npm run server:dev:hmr"
  },
  "dependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@angular/router": "^3.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.0-beta.12",
    "weui": "^0.4.3",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.32",
    "@types/jasmine": "^2.2.33",
    "@types/node": "^6.0.38",
    "@types/webpack": "^1.12.34",
    "autoprefixer": "^6.4.1",
    "awesome-typescript-loader": "~2.2.4",
    "codelyzer": "0.0.28",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.0-rc",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^0.14.1"
  }
}
